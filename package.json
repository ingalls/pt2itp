{
  "name": "pt2itp",
  "version": "9.9.0",
  "description": "Attach interpolation values given a road network and address points",
  "main": "index.js",
  "bin": {
    "pt2itp": "./index.js"
  },
  "scripts": {
    "doc": "./node_modules/documentation/bin/documentation.js lint lib/** && ./node_modules/documentation/bin/documentation.js build lib/** -f html -o docs",
    "lint": "eslint index.js test/* lib/*",
    "coverage": "istanbul cover tape test/*.js && coveralls < ./coverage/lcov.info",
    "test": "tape test/*.test.js",
    "pretest": "echo 'DROP DATABASE pt_test; CREATE DATABASE pt_test;' | psql -U postgres && echo 'CREATE EXTENSION postgis' | psql -U postgres pt_test"
  },
  "author": "Nicholas Ingalls <nicholas.ingalls@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@mapbox/carmen": "22.1.0",
    "@mapbox/tile-cover": "^3.0.2",
    "@mapbox/tilebelt": "^1.0.1",
    "@turf/turf": "4.1.0",
    "d3-queue": "3.0.5",
    "express": "^4.15.2",
    "lodash": "^4.2.0",
    "mbtiles": "^0.9.0",
    "minimist": "^1.2.0",
    "pg": "^6.1.0",
    "pg-cursor": "^1.0.1",
    "progress": "^2.0.0",
    "turf-line-slice-at-intersection": "^1.0.1",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "tape": "^4.2.0",
    "eslint": "^3.19.0",
    "coveralls": "^2.11.4",
    "documentation": "^4.0.0-rc.1",
    "istanbul": "^0.4.0"
  },
  "eslintConfig": {
    "rules": {
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "space-in-parens": 2,
      "space-before-blocks": 2,
      "keyword-spacing": 2,
      "comma-style": 2,
      "no-lonely-if": 2,
      "no-else-return": 0,
      "new-cap": 0,
      "no-empty": 2,
      "no-new": 2,
      "no-multi-spaces": 0,
      "space-in-brackets": 0,
      "brace-style": 0,
      "quotes": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "no-constant-condition": 0,
      "no-native-reassign": 0,
      "no-shadow": 0,
      "key-spacing": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "engines": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git"
  }
}
