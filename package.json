{
  "name": "pt2itp",
  "version": "19.3.0",
  "license": "BSD-2-Clause ",
  "description": "Attach interpolation values given a road network and address points",
  "main": "index.js",
  "bin": {
    "pt2itp": "./index.js"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "build": "neon build --release",
    "lint": "eslint index.js test/ lib/",
    "package": "neon build --release && node-pre-gyp package && node-pre-gyp-github publish",
    "coverage": "nyc tape test/*.js && nyc report --reporter=text-lcov > coverage.lcov",
    "coverage-upload": "coveralls < ./coverage.lcov",
    "doc": "./node_modules/documentation/bin/documentation.js lint ./lib/**",
    "test": "tape test/*.test.js",
    "pretest": "echo 'DROP DATABASE pt_test; CREATE DATABASE pt_test;' | psql postgres://postgres:@localhost:5432 && echo 'CREATE EXTENSION postgis;' | psql postgres://postgres:@localhost:5432/pt_test"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/ingalls/pt2itp/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native/",
    "pkg_path": "."
  },
  "author": "Nicholas Ingalls <nicholas.ingalls@gmail.com>",
  "dependencies": {
    "@mapbox/carmen": "26.2.0",
    "@mapbox/geocoder-abbreviations": "2.4.0",
    "@mapbox/geojson-area": "^0.2.2",
    "@mapbox/mbtiles": "^0.10.0",
    "@mapbox/tile-cover": "^3.0.2",
    "@mapbox/tilebelt": "^1.0.1",
    "@mapbox/title-case": "^1.3.0",
    "@turf/turf": "^5.1.0",
    "csv-stringify": "^5.0.0",
    "d3-queue": "^3.0.7",
    "diacritics": "^1.3.0",
    "express": "^4.15.3",
    "fast-csv": "^2.4.0",
    "fast-levenshtein": "^2.0.6",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "n-readlines": "^1.0.0",
    "neon-cli": "^0.2.0",
    "node-pre-gyp": "~0.10.0",
    "node-pre-gyp-github": "^1.4.3",
    "pg": "^7.3.0",
    "pg-copy-streams": "^1.2.0",
    "pg-cursor": "^2.0.0",
    "pluralize": "^7.0.0",
    "progress": "^2.0.0",
    "simple-statistics": "^7.0.0",
    "split": "^1.0.0",
    "talisman": "^0.21.0",
    "turf-line-slice-at-intersection": "^1.0.1",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "documentation": "^9.1.0",
    "eslint": "^5.3.0",
    "nyc": "^13.0.1",
    "tape": "^4.7.0",
    "tape-spawn": "^1.4.2",
    "tmp": "0.0.33"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "require-jsdoc": [
        "error",
        {
          "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": false,
            "ClassDeclaration": false,
            "ArrowFunctionExpression": false
          }
        }
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "no-tabs": 2,
      "space-in-parens": 2,
      "space-before-blocks": 2,
      "keyword-spacing": 2,
      "comma-style": 2,
      "no-lonely-if": 2,
      "no-else-return": 0,
      "new-cap": 2,
      "no-empty": 2,
      "no-trailing-spaces": 2,
      "no-new": 2,
      "no-multi-spaces": 2,
      "brace-style": 0,
      "quotes": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "no-constant-condition": 0,
      "no-native-reassign": 0,
      "no-shadow": 0,
      "key-spacing": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingalls/pt2itp.git"
  }
}
