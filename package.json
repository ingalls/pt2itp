{
  "name": "pt2itp",
  "version": "2.8.2",
  "description": "Attach interpolation values given a road network and address points",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js lib",
    "coverage": "istanbul cover tape test/*.js && coveralls < ./coverage/lcov.info",
    "test": "npm run lint && tape test/*.test.js"
  },
  "author": "Nicholas Ingalls <nicholas.ingalls@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cheap-ruler": "^2.4.1",
    "d3-queue": "3.0.1",
    "eslint": "2.13.1",
    "linematch": "^1.0.3",
    "lodash": "^4.2.0",
    "mapnik": "3.5.13",
    "minimist": "^1.2.0",
    "tile-cover": "^3.0.1",
    "tile-reduce": "3.1.1",
    "tilebelt": "^1.0.1",
    "turf": "3.0.11",
    "turf-feature": "^1.0.0"
  },
  "devDependencies": {
    "tape": "^4.2.0",
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0"
  },
  "eslintConfig": {
    "rules": {
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "space-in-parens": 2,
      "space-before-blocks": 2,
      "keyword-spacing": 2,
      "comma-style": 2,
      "no-lonely-if": 2,
      "no-else-return": 0,
      "new-cap": 0,
      "no-empty": 2,
      "no-new": 2,
      "no-multi-spaces": 0,
      "space-in-brackets": 0,
      "brace-style": 0,
      "quotes": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "no-constant-condition": 0,
      "no-native-reassign": 0,
      "no-shadow": 0,
      "key-spacing": 0
    },
    "env": {
      "node": true
    }
  },
  "engines": {
    "node": "4.2.3"
  },
  "repository": {
    "type": "git"
  }
}
